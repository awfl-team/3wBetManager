{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "no-nested-ternary": 0,
    "react/prefer-stateless-function": 0,
    "prefer-destructuring": 0,
    "react/jsx-filename-extension": 0,
    "react/no-unescaped-entities": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0
  },
  "env": {
    "browser": true
  }
}
